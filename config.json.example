{
  "llm_integration": {
    "provider": "openai",
    "api_key": "your-api-key-here",
    "model": "gpt-4o",
    "fallback_to_roo": false,
    "options": {
      "temperature": 0.7,
      "max_tokens": 4000,
      "timeout": 120,
      "azure_endpoint": "",
      "api_version": "2024-02-01",
      "ollama_host": "http://localhost:11434"
    },
    "_provider_examples": {
      "openai": {
        "provider": "openai",
        "api_key": "sk-proj-...",
        "model": "gpt-4o"
      },
      "anthropic": {
        "provider": "anthropic", 
        "api_key": "sk-ant-api03-...",
        "model": "claude-3-5-sonnet-20241022"
      },
      "google": {
        "provider": "google",
        "api_key": "AI...", 
        "model": "gemini-1.5-pro"
      },
      "azure_openai": {
        "provider": "azure_openai",
        "api_key": "your-azure-key",
        "model": "gpt-4o",
        "options": {
          "azure_endpoint": "https://your-resource.openai.azure.com/",
          "api_version": "2024-02-01"
        }
      },
      "ollama": {
        "provider": "ollama",
        "model": "llama3.2",
        "options": {
          "ollama_host": "http://localhost:11434"
        }
      },
      "roo_code_fallback": {
        "provider": "roo_code",
        "fallback_to_roo": true
      }
    }
  },
  "processing": {
    "output_directory": "data",
    "date_range_months": 12
  }
}